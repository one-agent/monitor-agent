# LLM 配置（必需）
LLM_API_KEY=your-openai-api-key-here
LLM_BASE_URL=https://api.openai.com/v1
LLM_MODEL_NAME=gpt-3.5-turbo

# Embedding 配置（用于 RAG）
EMBEDDING_API_KEY=your-openai-api-key-here
EMBEDDING_BASE_URL=https://api.openai.com/v1
EMBEDDING_MODEL_NAME=text-embedding-3-small

# 飞书 Webhook（可选）
FEISHU_WEBHOOK_URL=https://open.feishu.cn/open-apis/bot/v2/hook/your-webhook-url

# Apifox API（可选）
APIFOX_API_URL=https://api.apifox.com
APIFOX_API_TOKEN=your-apifox-token
APIFOX_PROJECT_ID=your-project-id-here
APIFOX_FOLDER_ID=your-folder-id-here
APIFOX_MODULE_ID=your-module-id-here
